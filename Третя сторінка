from PyQt5.QtCore import Qt
from PyQt5.QtWidgets import QApplication, QWidget, QPushButton, QLabel,QHBoxLayout, QVBoxLayout, QGroupBox,QRadioButton, QListWidget, QLineEdit
from instr import *

class FinalWin(QWidget):
    def __init__(self):
        super().__init__()
        self.set_appear()
        self.iniUI()
        self.show()

    def iniUI(self):
        self.helllo_text=QLabel(txt_index)
        self.indi=QLabel(txt_workheart)

        self.layout_line2=QVBoxLayout()  
        self.layout_line2.addWidget(self.helllo_text, alignment = Qt.AlignCenter)  
        self.layout_line2.addWidget(self.indi, alignment = Qt.AlignCenter)
        self.setLayout(self.layout_line)

    def set_appear(self):
        self.setWindowTitle(txt_finalwin)
        self.resize(win_width, win_height)
        self.move(win_x, win_y)
